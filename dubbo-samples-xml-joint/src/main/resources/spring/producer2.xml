<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:provider retries="3" timeout="5000" loadbalance="random" />
    <dubbo:application name="dubbo-producer" />
    <dubbo:registry address="zookeeper://localhost:2181" />
    <dubbo:protocol name="dubbo" port="20881" />
    <bean id="greetingService" class="com.maslke.dubbo.samples.xml.joint.service.impl.GreetingServiceImpl" />
    <dubbo:service interface="com.maslke.dubbo.samples.xml.joint.api.GreetingService" ref="greetingService"
                   version="2.0" />
</beans>